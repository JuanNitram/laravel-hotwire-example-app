{
  "settings": {},
  "rules": [
    {
      "patterns": [
        "/dashboard$"
      ],
      "properties": {
        "uri": "hotwire://fragment/web/home",
        "presentation": "push",
        "pull_to_refresh_enabled": true,
        "cache": true
      }
    },
    {
      "patterns": [
        "/settings$"
      ],
      "properties": {
        "uri": "hotwire://fragment/web/home",
        "presentation": "push",
        "pull_to_refresh_enabled": true,
        "cache": true
      }
    },
    {
      "patterns": [
        "/login$"
      ],
      "properties": {
        "uri": "hotwire://fragment/web",
        "presentation": "replace_root",
        "pull_to_refresh_enabled": false
      }
    },
    {
      "patterns": [
        "/create/?$",
        "/edit/?$",
        "/delete/?$"
      ],
      "properties": {
        "context": "modal",
        "uri": "hotwire://fragment/web",
        "presentation": "push",
        "pull_to_refresh_enabled": false
      }
    },
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "uri": "hotwire://fragment/web/home",
        "presentation": "push",
        "pull_to_refresh_enabled": true,
        "cache": true
      }
    }
  ]
}