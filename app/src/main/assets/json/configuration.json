{
  "settings": {},
  "rules": [
    {
      "patterns": [
        "^/dashboard$",
        "^/settings$"
      ],
      "properties": {
        "uri": "hotwire://fragment/web/home",
        "pull_to_refresh_enabled": false
      }
    },
    {
      "patterns": [
        "^/products$"
      ],
      "properties": {
        "uri": "hotwire://fragment/web/home",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "^/settings/.+"
      ],
      "properties": {
        "context": "modal",
        "uri": "hotwire://fragment/web",
        "pull_to_refresh_enabled": false,
        "presentation": "modal"
      }
    },
    {
      "patterns": [
        "/login"
      ],
      "properties": {
        "uri": "hotwire://fragment/web",
        "pull_to_refresh_enabled": false
      }
    }
  ]
}